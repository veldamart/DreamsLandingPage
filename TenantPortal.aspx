    // extend moment with add by business days function
    moment.fn.businessAdd = function (days) {
        var sign = days < 0 ? -1 : 1;
        days = Math.abs(days);
        var i = 0;
        while (i < days) {
            this.add(sign, 'd');
            if (this.day() > 0 && this.day() < 6) {
                i++;
            }
        }
        return this;
    };
